{
  "name": "food-ordering-fullstack",
  "version": "1.0.0",
  "description": "Monorepo for Food Ordering App",
  "scripts": {
    "start-frontend": "npm run dev --prefix client",
    "start-backend": "npm run dev --prefix server",
    "start-all": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "install-all": "npm install --prefix client && npm install --prefix server"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}